# NetBird API Configuration
NETBIRD_API_URL=https://api.netbird.io
NETBIRD_API_TOKEN=your_netbird_api_token_here

# Exporter Configuration
LISTEN_ADDRESS=:8080
METRICS_PATH=/metrics
LOG_LEVEL=info

# Optional Exporters (high cardinality metrics - disabled by default)
# Enable these only if you need detailed metrics and understand the cardinality implications
ENABLE_TOKENS_EXPORTER=false
ENABLE_EVENTS_EXPORTER=false
